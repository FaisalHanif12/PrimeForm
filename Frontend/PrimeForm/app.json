{
  "expo": {
    "name": "Pure Body",
    "slug": "purebody",
    "version": "1.0.0",
    "orientation": "portrait",
    "developmentClient": {
      "silentLaunch": true
    },
    "icon": "assets/images/PrimeIcon.png",
    "scheme": "purebody",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maherfaisal.purebody",
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "notification": {
      "icon": "./assets/images/PrimeIcon.png",
      "color": "#6366F1",
      "androidMode": "default",
      "androidCollapsedTitle": "Pure Body"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/PrimeIcon.png",
        "backgroundColor": "#1a1a2e"
      },
      "edgeToEdgeEnabled": true,
      "notification": {
        "icon": "./assets/images/PrimeIcon.png",
        "color": "#6366F1"
      },
      "package": "com.maherfaisal.purebody"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "assets/images/PrimeIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/PrimeIcon.png",
          "color": "#6366F1",
          "defaultChannel": "purebody-notifications"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8fd8d9d9-0e37-4002-b75e-545f7a2fc230"
      }
    }
  }
}
