{
  "expo": {
    "name": "PrimeForm",
    "slug": "PrimeForm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/PrimeIcon1.png",
    "scheme": "primeform",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/PrimeIcon1.png",
        "backgroundColor": "#1a1a2e"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "assets/images/PrimeIcon1.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
