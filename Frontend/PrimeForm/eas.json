{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "image": "latest",
        "env": {
          "GRADLE_OPTS": "-Dorg.gradle.jvmargs=-Xmx6144m -Dkotlin.daemon.jvm.options=-Xmx2048m -Dandroid.experimental.art-profile-r8-rewriting=false"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "image": "latest",
        "env": {
          "GRADLE_OPTS": "-Dorg.gradle.jvmargs=-Xmx6144m -Dkotlin.daemon.jvm.options=-Xmx2048m -Dandroid.experimental.art-profile-r8-rewriting=false",
          "EXPO_PUBLIC_ADS_MODE": "test"
        }
      }
    },
    "preview-debug": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "image": "latest",
        "env": {
          "GRADLE_OPTS": "-Dorg.gradle.jvmargs=-Xmx6144m -Dkotlin.daemon.jvm.options=-Xmx2048m -Dandroid.experimental.art-profile-r8-rewriting=false",
          "EXPO_PUBLIC_ADS_MODE": "test",
          "EXPO_PUBLIC_DEBUG_PUSH": "true",
          "EXPO_PUBLIC_DEBUG_ADS": "true"
        }
      }
    },
    "production": {
      "channel": "production",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle",
        "image": "latest",
        "env": {
          "GRADLE_OPTS": "-Dorg.gradle.jvmargs=-Xmx6144m -Dkotlin.daemon.jvm.options=-Xmx2048m -Dandroid.experimental.art-profile-r8-rewriting=false"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
